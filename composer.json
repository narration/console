{
    "name": "narration/testing",
    "description": "The Narration Framework.",
    "keywords": [
        "narration",
        "php",
        "framework"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.11",
        "narration/phpstan-rules": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Narration\\Testing\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
